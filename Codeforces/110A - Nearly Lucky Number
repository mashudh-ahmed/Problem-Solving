#include <bits/stdc++.h>
using namespace std;
 
typedef  long long ll;
 
int main()
{
    string n;
    cin>>n;
    
    ll length = n.size();
    ll count = 0;
    
    for (int i = 0; i< length; i++)
    {
        if ( n[i] == '4' || n[i] == '7')
        {
            count++;
        }
    }
    if (count == 4 || count == 7)
    {
        cout<< "YES"<<endl;
    }
    else 
    {
        cout<< "NO"<<endl;
    }
    
return 0;    
}
