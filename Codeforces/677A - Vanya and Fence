#include <bits/stdc++.h>
using namespace std;

int main() {
    
   int n,h;
   cin>>n>>h;
   
   int A[n];
   
   int count = 0;
   
   for (int i=0; i<n; i++)
   {
       cin>> A[i];
       
       if (A[i] > h)
       {
           count+=2;
       }
       else 
       {
           count +=1;
       }
   }
 
    cout<< count<<endl;
    
    return 0;
}
